{
    "name": "Facebook by DedoCibula",
    "version": "2.0",
    "manifest_version": 2,
    "description": "Keep up with your friends, even when you're not on Facebook.",
    "browser_action": {
        "default_icon": "images/icon19.png",
        "default_title": "Facebook Extension"
    },
    "background": {
        "scripts": [ "Scripts/jquery-2.2.0.min.js", "entities.js", "background.js" ]
    },
    "permissions": [ "http://*.facebook.com/*", "https://*.facebook.com/*", "webRequest", "webRequestBlocking", "notifications" ],
    "icons": {
        "128": "Images/icon128.png",
        "16": "Images/icon16.png",
        "32": "Images/icon32.png",
        "48": "Images/icon48.png"
    }
}
